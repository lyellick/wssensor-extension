{
  "manifest_version": 3,
  "name": "WS Sensor Extension",
  "version": "0.1.0",
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "WS Sensor Extension",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}